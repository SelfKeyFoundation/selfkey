{
  "name": "selfkey-token",
  "version": "0.2.1",
  "description": "The SelfKey Token is an ERC20 standard token that is used to fuel the SelfKey KYC dapps",
  "main": "truffle.js",
  "directories": {
    "test": "test",
    "app": "app",
    "contracts": "contracts"
  },
  "engines": {
    "node": "9.2.0",
    "npm": "5.5.1"
  },
  "dependencies": {
    "bip39": "^2.4.0",
    "ethereumjs-wallet": "^0.6.0",
    "web3": "1.0.0-beta.26",
    "web3-provider-engine": "^13.3.3",
    "zeppelin-solidity": "^1.4.0"
  },
  "devDependencies": {
    "eslint": "^4.12.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "faker": "^4.1.0",
    "ganache-cli": "^6.0.3",
    "solhint": "^1.1.8",
    "solidity-coverage": "^0.4.2"
  },
  "scripts": {
    "testrpc": "ganache-cli --port 8545 --unlock 0",
